                {{/if}}
                <!--Custom link 1-->
                {{#if settings.dc_toggle}}
                  <a href="{{dc 'header_url_link_1'}}" class="header_url_link hide-on-mobile">{{dc 'header_url_title_1'}}</a>
                {{else}}
                  <a href="{{settings.header_url_link_1}}" class="header_url_link hide-on-mobile">{{settings.header_url_title_1}}</a>
                {{/if}}
                <!--Custom link 2-->
                {{#if settings.dc_toggle}}
                  <a href="{{dc 'header_url_link_2'}}" class="header_url_link hide-on-mobile">{{dc 'header_url_title_2'}}</a>
                {{else}}
                  <a href="{{settings.header_url_link_2}}" class="header_url_link hide-on-mobile">{{settings.header_url_title_2}}</a>
                {{/if}}
                <!--Custom link 3-->
                {{#if settings.dc_toggle}}
                  <a href="{{dc 'header_url_link_3'}}" class="header_url_link hide-on-mobile">{{dc 'header_url_title_3'}}</a>
                {{else}}
                  <a href="{{settings.header_url_link_3}}" class="header_url_link hide-on-mobile">{{settings.header_url_title_3}}</a>
                {{/if}}
                <!--Custom link 4-->
                {{#if settings.dc_toggle}}
                  <a href="{{dc 'header_url_link_4'}}" class="header_url_link hide-on-mobile">{{dc 'header_url_title_4'}}</a>
                {{else}}
                  <a href="{{settings.header_url_link_4}}" class="header_url_link hide-on-mobile">{{settings.header_url_title_4}}</a>
                {{/if}}
                <!--Custom link 5-->
                {{#if settings.dc_toggle}}
                  <a href="{{dc 'header_url_link_5'}}" class="header_url_link hide-on-mobile">{{dc 'header_url_title_5'}}</a>
                {{else}}
                  <a href="{{settings.header_url_link_5}}" class="header_url_link hide-on-mobile">{{settings.header_url_title_5}}</a>
                {{/if}}
				<!--Custom link 6-->
                {{#if settings.dc_toggle}}
                  <a href="{{dc 'header_url_link_6'}}" class="header_url_link hide-on-mobile">{{dc 'header_url_title_6'}}</a>
                {{else}}
                  <a href="{{settings.header_url_link_6}}" class="header_url_link hide-on-mobile">{{settings.header_url_title_6}}</a>
                {{/if}}
                {{#if settings.sign_in_toggle}}
                  {{#if signed_in}}
                    <div class="user-info dropdown hide-on-mobile">
                      <button class="dropdown-toggle" aria-haspopup="true">
                        {{user_avatar class="user-avatar"}}
                        <span class="hide-on-mobile">
                          {{user_name}}
                        </span>
                      </button>
                      <div class="dropdown-menu dropdown-header" role="menu">
                        {{link "my_activities" role="menuitem"}}
                        {{my_profile role="menuitem"}}
                        {{change_password role="menuitem"}}
                        {{link "sign_out" role="menuitem"}}
                      </div>
                    </div>
                  {{else}}
                    {{#link "sign_in" class="sign-in hide-on-mobile"}}
