  <script>
    $(document).ready(function() {
      let owlOptions = {
        items: 1,
        dots: false,
        nav: true,
        loop: true,
        center: true,
        autoplay: {{settings.info_notification_timeout}}
      }
      $("#info-notification-carousel").owlCarousel(owlOptions);
      $('#_samewidth_images').owlCarousel({
        margin: 10,
        autoWidth: false,
        nav: true,
        items: 4,
        navText: ['<i class="fa fa-angle-left" aria-hidden="true"></i>', '<i class="fa fa-angle-right" aria-hidden="true"></i>']
      })
    });
  </script>

  <!-- Dialog Contents -->

  <div id="findModel" style="max-width: 600px; display: none;">
    <h2 class="font-bold">Find My Model Number</h2>
    <div style="width: 100%;">
      <div>
        {{!-- <h4 class="font-bold">1. Check your box</h4> --}}
        <p>1. Check the bottom of your box, you should see the model number near the bar code.</p>
      </div>
      <div>
        {{!-- <h4 class="font-bold">2. Check your unit</h4> --}}
        <p>2. Check the main body of the vacuum (back of the unit) it should have a rating label that features your model number.
        </p>
      </div>
      <div>
        {{!-- <h4 class="font-bold">3. Check your manual</h4> --}}
        <p>3. Check your manual. Your product's model number is listed on the front cover of your instruction booklet.</p>
      </div>
    </div>
    {{!-- <div  style="width: 100%; text-align:center;"><img src="https://www.sharkclean.com/include/images/support/model.jpg"
        alt="Where to find the model number" style="width: 100%;"></div> --}}
  </div>
  
