{{#if settings.info_notification_toggle}}
  <div class="info-header">
    <div class="info-banner">
      <div class="container">
        <div class="banner-header">
          <div id="info-notification-carousel" class="owl-carousel owl-theme" style="overflow: hidden;">
            {{#isnt settings.info_notification_body_1 ""}}
            <h2 style="margin-bottom: 0;" class="info-body text-center">
              {{settings.info_notification_body_1}}
              <a href="{{settings.info_notification_url_1}}" target="_blank">{{settings.info_notification_link_1}}</a>
            </h2>
            {{/isnt}}
            {{#isnt settings.info_notification_body_2 ""}}
            <h2 style="margin-bottom: 0;" class="info-body text-center">
              {{settings.info_notification_body_2}}
              <a href="{{settings.info_notification_url_2}}" target="_blank">{{settings.info_notification_link_2}}</a>
            </h2>
            {{/isnt}}
            {{#isnt settings.info_notification_body_3 ""}}
            <h2 style="margin-bottom: 0;" class="info-body text-center">
              {{settings.info_notification_body_3}}
              <a href="{{settings.info_notification_url_3}}" target="_blank">{{settings.info_notification_link_3}}</a>
            </h2>
            {{/isnt}}
          </div>
        </div>
      </div>
    </div>
  </div>
{{/if}}
<div class="wrapper">
  <div class="wrapper-header">
    <div class="searchform header">
      <img src="{{asset 'Search.svg'}}" class="searchform-icon">{{search instant=true submit=false placeholder=settings.search_placeholder_text}}&nbsp;<a id="closeSearch"><i class="fas fa-times"></i></a>
    </div>
    <header id="site-header" class="header header-has-background" data-header>
      <div class="container">
        <div class="header-wrapper">
          <div class="header-col clearfix">
            <div class="logo-wrapper">
              <div class="logo">
                {{#link 'help_center'}}
