            {{/if}}
          </div>
		  <!--Custom link 6-->
          <div class="sidebar_mobile_item">
            {{#if settings.dc_toggle}}
              <a href="{{dc 'header_url_link_6'}}" class="header_url_title">{{dc 'header_url_title_6'}}</a>
            {{else}}
              <a href="{{settings.header_url_link_6}}" class="header_url_title">{{settings.header_url_title_6}}</a>
            {{/if}}
          </div>
          <div class="sidebar_mobile_item">
            {{#if signed_in}}
