                <img src="{{settings.logo_secondary}}" alt="{{t 'logo'}}">
                {{/link}}
              </div>
            </div>
            {{!-- {{#if settings.help_center_title_toggle}}
              <p class="hc-name">
                {{#if settings.dc_toggle}}
                  <a class="" href="/">
                    {{dc 'help_center_title'}}
                  </a>
                {{else}}
                  <a class="" href="/">
                    {{settings.help_center_title}}
                  </a>
                {{/if}}
              </p>
            {{/if}} --}}
            <div class="searchbar-collapse">
            </div>
          </div>
          <div class="header-col header-menu">
            <div class="header-collapse" data-menu>
              <div class="header-panel">
                {{!-- {{#if settings.searchbar_transparent_toggle}} --}}
                  <div class="searchbar-top transparent">
                    {{search instant=true submit=false placeholder=settings.search_placeholder_text class='search search-full'}}
                    <div class="searchbar-info" data-fancybox data-src="#findModel">
                      <i class="fa fa-info-circle"></i>
                    </div>
                  </div>
                {{!-- {{else}}
                  <div class="searchbar-top">
                    {{search instant=true submit=false placeholder=settings.search_placeholder_text class='search search-full'}}
